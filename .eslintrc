{
  "parser": "babel-eslint",
  "plugins": ["react"],
  "extends": "airbnb",
  "rules": {
    "arrow-parens": 0,
    "class-methods-use-this": 0,
    "jsx-a11y/no-static-element-interactions": 0,
    "object-curly-spacing": [2, "never"],
    "quotes": [1, "double", "avoid-escape"],
    "react/forbid-prop-types": 0,
    "react/jsx-filename-extension": 0,
    "react/jsx-indent": 0,
    "semi": [2, "never"]
  }
}
